<solver>
    <constructionHeuristic/>
    <localSearch>
        <unionMoveSelector>
            <changeMoveSelector/>
            <swapMoveSelector/>
            <!-- To escape the local optimum in knapsack, add sub pillar swap moves. -->
            <pillarSwapMoveSelector>
                <subPillarType>ALL</subPillarType>
            </pillarSwapMoveSelector>
        </unionMoveSelector>
    </localSearch>
    <environmentMode>NON_REPRODUCIBLE</environmentMode>
</solver>
